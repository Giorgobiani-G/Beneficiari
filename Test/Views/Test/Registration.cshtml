@model Beneficiari
@{
    ViewData["Title"] = "Registration";
}

<h1>ახალი ბენეფიციარი</h1>

<div class="row">
    <form method="post" class="form-horizontal" role="form">
        <div class="text-danger"></div>

        <div class="form-group">
            <label asp-for="Tarigi" class="col-sm-3 control-label">თარიღი</label>
            <div class="col-sm-12">
                <input asp-for="Tarigi" type="date" id="piradoba" placeholder="თარიღი" class="form-control" autofocus>

            </div>
        </div>


        <div class="form-group">
            <label asp-for="Piradobisnomeri" class="col-sm-3 control-label">პ/ნ</label>
            <div class="col-sm-12">
                <input asp-for="Piradobisnomeri" type="text" id="piradoba" placeholder="პ/ნ" class="form-control" autofocus>

            </div>
        </div>

        <div class="form-group">
            <label asp-for="Saxeli" class="col-sm-3 control-label">სახელი</label>
            <div class="col-sm-12">
                <input asp-for="Saxeli" type="text" id="firstName" placeholder="სახელი" class="form-control" autofocus>

            </div>
        </div>
        <div class="form-group">
            <label asp-for="Gvari" class="col-sm-3 control-label">გვარი</label>
            <div class="col-sm-12">
                <input asp-for="Gvari" type="text" id="gvar" placeholder="გვარი" class="form-control" autofocus>

            </div>
        </div>

        <div class="form-group">
            <label asp-for="DabTarigi" class="col-sm-3 control-label">დაბ.თარიღი</label>
            <div class="col-sm-12">
                <input asp-for="DabTarigi" type="date" id="dabtarigi" placeholder="დაბ.თარიღი" class="form-control" autofocus>

            </div>
        </div>



        @*<div class="form-group">
            <label asp-for="Asaki" class="col-sm-3 control-label">ასაკი</label>
            <div class="col-sm-12">
                <input asp-for="Asaki" type="text" id="asaki" placeholder="ასაკი" class="form-control" autofocus>

            </div>
        </div>*@
        <div class="form-group">
            <label asp-for="Misamarti" class="col-sm-3 control-label">მისამართი</label>
            <div class="col-sm-12">
                <input asp-for="Misamarti" type="text" id="mis" placeholder="მისამართი" class="form-control" autofocus>

            </div>
        </div>

        <div class="form-group">
            <label asp-for="Telefoni" class="col-sm-3 control-label">ტელეფონი</label>
            <div class="col-sm-12">
                <input asp-for="Telefoni" type="text" id="mis" placeholder="ტელეფონი" class="form-control" autofocus>

            </div>
        </div>





        <button type="submit" class="btn btn-success">Register</button>
        <button asp-action="Index" class="btn btn-danger">Cancel</button>


    </form>

</div>
